* Thursday, February 18, 1993       2:03:23 PM*  this is a segment of the equates file*	obviously the numbering starts at $80 and jumps around a bit* it should still be useful to you as you listen to the tape*** BOB SOUNDS*SFXGUN          EQU     $81     ;;;   FIRE GUN	SFXUZI          EQU     $82     ;;;   FIRE TRIPLE SHOT SFXFLAME        EQU     $83     ;;;   FIRE FLAMESFXMISSILE      EQU     $84     ;;;   FIRE MISSILESFXBEAM         EQU     $85     ;;;   FIRE PULSE BEAMSFXSONIC        EQU     $86     ;;;   FIRE SONIC BOOM								SFXSWITCH       EQU     $87       ;;;   CHANGE WEAPON CLICKSFXDEPLOYREM    EQU     $88       ;;;   OPEN HATCH DOOR FOR REMOTESFXPUNCH        EQU     $89       ;;;   PUNCH WHOOSH (louder)SFXSKID         EQU     $8a       ;;;   SMALL SKID FOR STOPPINGSFXCROUCH       EQU     $8b       ;;;   SQUISHING DOWNSFXCLIMB	    EQU     $8c       ;;;   CLIMBING ON LADDERSFXFINGER       EQU     $8d       ;;;   HAND OVER HAND (shorter 1/2)SFXSCOOTER      EQU     $8e		  ; SCOOTER ENGINE - doppler whoosh       SFXCARENGINE    EQU     $8f		  ; Old style Piston engine        SFXSLEEP        EQU     $90       ;;;   SNORING ZZZZSFXPICKUP       EQU     $91       ;;; GET OBJECT (shorter)SFXPOWERUP      EQU     $92       ;;; ENERGY RECHARGE (shorter)SFXBOBHIT1      EQU     $93       ;OOOF - VOICESFXMELTDEATH   	EQU     $94       ;Bob meltsSFXCRUMBLEDEATH EQU     $95       ;Bob crumblesSFXBOBSPLAT1    EQU     $96       ;;; HIT WALL FACE FIRSTSFXBOBSPLAT2    EQU     $97       ;;; BUTT SPLATSFXELEVATOR     EQU     $cd       ;;; ELEVATOR ACTIVATE - (turn on only)** REMOTE SOUNDS*SFXLIGHT        EQU     $98       ;;;   FLASH SOUNDSFXSHIELD       EQU     $99       ;;;	HUMMMM..(make more electric)SFXSPRING       EQU     $9a       ;;;   BOINGG..(remove second half)SFXCOPTER       EQU     $9b       ;;;   CHUGGA.CHUGGA.CHUGGA (too quiet)SFXPARACHUTE   EQU      $9c       ; (parachute opening)SFXBEZERKER     EQU     $9d       ;;;   TICK.TICK..BOOOM!** GENERIC SOUNDS*SFXEXPLODE1     EQU     $9e     ;;; 	SMALL EXPLOSION (make all bigger)SFXEXPLODE2     EQU     $9f     ;;;     BIGGER EXPLOSION SFXBUGSPLAT1    EQU     $a0    ;;;     SMALL BUG SPLATSFXBUGSPLAT2    EQU     $a1     ;;;	    LARGER BUG SPLATSFXNUKEOUT		EQU		$a2		;NUKE-REZ OUTSFXFLAMEOUT		EQU		$a3		;FLAME DEATHSFXTELEPORT     EQU     $a4     ;;;  HUM AS BOB REZES IN/OUT OF LEVELS								;(remove end bit)								SFXCRASH1       EQU     $a5     ;SMALL THUDSFXCRASH2       EQU     $a6     ;LARGER THUD - CRUSH WALLSFXCRASH3		EQU		$9f		;VERY LARGE THUD - Dropping ball** BORG SOUNDS*SFXBORGFIRE1    EQU     $a7       ;> BORG FIRES WEAPON - BULLETSFXBORGFIRE2    EQU     $a8       ;> BORG FIRES WEAPON - BOMMERANGSFXBORGFIRE3    EQU     $a9       ;> BORG FIRES WEAPON - BIG BULLETSFXBORGFIRE4    EQU     $aa       ;> BORG FIRES WEAPON - OTHER THINGYSFXTURRETFIRE   EQU     $ab       ;;; WALL TURRETS FIRESFXMISSILE1     EQU     $ac       ;HORIZONTAL SLOW MISSILESSFXMISSILE2     EQU     $ad       ;FAST WHOOSH MISSILESSFXGASJET       EQU     $ae       ;;; STEAM JETS FROM PIPESSFXSNAKEBOSS1	EQU		$af		  ;> SNAKE BOSS SHRIEK - HIGHSFXSNAKEBOSS2	EQU		$b0	      ;> SNAKE BOSS SHRIEK - LOWSFXSNAKEBOSS3	EQU		$b1		  ;> SNAKE BOSS SHRIEK - FIRE BULLET; DOOR LIFTING HUGE BORG BOSSSFXDOOR1		EQU		$b2			;Ã BIG,DEEP HEARTY LAUGHSFXDOOR2		EQU		$b3			;Ã PUNCH OUT HOLES THROUGH DOORSFXDOOR3		EQU		$b4			;Ã LIFT DOOR UP (1 SECOND)SFXDOOR4		EQU		$b5			;Ã POUND FIST DOWN AT BOB** BUG SOUNDS AND ULTRA FORCE WORLD 3 SOUNDS*SFXHEADDROP     EQU     $b6       ;Ã HEAD EATER DROP OFF CEILINGSFXHEADEAT      EQU     $b7       ;Ã HEAD EATER ATTACHEDSFXROLYPOLY     EQU     $b8       ;Ã BOWLING BALLSFXSCUTTLE      EQU     $b9       ;Ã SCORPION /SANDCRAB SCUTTLESFXWHIP         EQU     $ba       ;Ã SCORPION WHIP TALE                        SFXSNAP			EQU		$bb		  ;Ã SANDCRAB CLAWS SNAP AT BOBSFXWINGS        EQU     $bc       ;Ã BUZZZ OF BIG FLIESSFXFLIES        EQU     $bd       ;Ã LITTLE FLIESSFXEGG          EQU     $be       ;Ã EGG SPLOTCHING OUT ROACHESSFXEGGOPEN		EQU		$bf		  ;Ã EGG OPENS UP SFXWORKER       EQU     $c0       ;Ã WORKER FIRES VENOM BALL, BUG SHOOT GENERALSFXQUEENSCREAM  EQU     $c1       ;Ã DYING ALIEN SHRIEKSFXDRIP			EQU		$c2		  ;Ã QUEEN DRIPPING GUTS; ULTRA FORCE SPECIFIC SOUNDSSFXFISHJUMP		EQU		$c3		;Ã FISH JUMPING OUT OF LIQUIDSFXNERVEPULSE	EQU		$c4		;Ã NERVE PULSE GATE ACTIVATESFXJELLYFIRE	EQU		$c5		;Ã JELLY CREATURE FIRESSFXEELFIRE		EQU		$c6		;Ã EEL FIRESSFXCLUSTER		EQU		$c7		;Ã SOUND WHEN CELL CLUSTER IS SHOT BY BOBSFXCELLMANFIRE	 EQU	$c8		;Ã CELL MAN SHOOTSSFXPUSSMINEBURST EQU	$c9		;Ã PUSS MINE EXPLODES INTO FOUR PIECESSFXMENACE		EQU		$ca		;Ã	LADDER MENACE CLIMBS UP/DOWN LADDERSFXPUSSDROP		EQU		$cb		;Ã	BALL OF PUSS DROPS FROM LITTLE CREATURESFXPUSSMANBELCH	EQU		$cc		;Ã	PUSS MAN BOSS VOMITS EMBRYOSFXPUSS2		EQU		$cc		;SFXPUSS3		EQU		$cb		;SFXPUSS4		EQU		$cc		;SFXPUSS5		EQU		$cb		;		;big boss is silent for now, until it's there to be seencbSFXBIGBOSS1		EQU		$cd		;FINAL BOSS ZOOM IN FROM DISTANCE (DOPPLER)SFXBIGBOSS2		EQU		$cd		;ZOOM OUTSFXBIGBOSS3		EQU		$cd		;BIG VOICE SAYING "BOOOO!"SFXBIGBOSS4		EQU		$cd		;SCREAM AS IT GETS SHOT TO BITSSFXBIGBOSS5		EQU		$cd		;"SCHLOOORP" AS INISDE PART STICKS TO WALLS;EMPTY			EQU		$cd		;** TEMPLE SOUNDS*SFXTHUNDER      EQU     $ce       ;Ã CRACK OF THUNDERSFXPOWERORB     EQU     $cf       ;Ã SHOOTING SOUND FOR POWER ORB BALLSSFXBOWS         EQU     $d0       ;Ã BOW RELEASESFXSCREAMHAND   EQU     $d1       ;Ã hand screamSFXEYEPOP       EQU     $d2       ;Ã EYE ON HAND IS SHOTSFXTOUNGE       EQU     $d3       ;Ã TONGUE SHOOTS OUT     SFXBALLDROP     EQU     $d4       ;Ã GIANT SPHERE DROPS ON GROUND THUDSFXPOTATO		EQU		$d5		  ;Ã PUNCH BY GIANT MR.POTATO HEAD SFXSHADOWMAN	EQU		$cd			;SHADOW MAN EMERGESSFXGOLDBALL		EQU		$d6			;GOLDEN BALLS ENTER/LEAVE LAVA;SFXFLOWER1		EQU		$82			;FLOWER BOSS FLOWER POPS OPEN;SFXFLOWER2		EQU		$82			;ARM FLINGS BOB;SFXFLOWER3		EQU		$82			;ARM SMASHED BOB INTO PANCAKE;SFXFLOWER4		EQU		$82			;SHOT-OFF ARM SPEWS GUNK INTO AIR;SFXFLOWER5		EQU		$82			;MUTOID HEAD EMRGES FROM FLOWER POD;SFXPOPEYE1		EQU		$82			;BOSS WALKS;SFXPOPEYE2		EQU		$82			;BOSS JUMPS;SFXPOPEYE3		EQU		$82			;BOSS LANDS;SFXPOPEYE4		EQU		$82			;BOSS TURNS HEAD AND SPITS AT BOB** LAVA SOUNDS*SFXFIREIMP      EQU     $d7        ;Ã FIRE IMP FLY BYSFXFIREBALLS    EQU     $d8        ;Ã BALLS EMERGING FROM LAVASFXFIRETHROW    EQU     SFXFIREBALLS ;Ã FIRE BALLS THROWN BY FIRE MANSFXLAVAMAN		EQU		$d9			;Ã LAVA MAN RISES FROM LAVA STREAMSSFXLAVABOSS		EQU		SFXLAVAMAN	;Ã LAVA BOSS RISES FROM LAVASFXLAVABOSSSHOOT EQU	SFXFIREBALLS	 ;Ã BOSS SHOOTS FIREBALLSFXVOLCANO		EQU		SFXFIREBALLS	;Ã VOLCANO SHOOTS FIREBALLS** ULTRA FOREST SOUNDS*SFXVENUSCHOMP		EQU		$da			;VENUS FLYTRAP CHOMPSSFXVENUSFIRE		EQU		$db			;VENUS FLYTRAP FIRESSFXBUBBLEFIRE		EQU		$dc			;BUBBLE MAN FIRES BULLETSFXBUBBLEDIE		EQU		$dd			;BUBBLE MAN EXPLODESSFXSHROOMPOP		EQU		$de			;MUSHROOM EXPLODES INTO SPORESSFXTREETURRET		EQU		SFXBUBBLEFIRE	;TREE TURRET FIRES BUBBLESFXFUNBALLBOUNCE	EQU		$df			;FUN BALL "BOING" BOUNCESFXFRUITDROP		EQU		$e0			;ROTTEN FRUIT DROPS FROM TREESFXDONUTATTACK		EQU		$e1		    ;FLYING DONUTS - "ATTACK" NOISE 